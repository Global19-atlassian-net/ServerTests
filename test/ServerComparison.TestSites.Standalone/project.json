{
  "version": "1.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.2",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.3",
    "Microsoft.AspNetCore.Server.WebListener": "1.0.3",
    "Microsoft.AspNetCore.WebUtilities": "1.0.2",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.2",
    "Microsoft.Extensions.Configuration.Json": "1.0.2",
    "Microsoft.Extensions.Logging.Console": "1.0.2",
    "Microsoft.Net.Http.Headers": "1.0.2"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": {
      "include": "../ServerComparison.TestSites/Startup*.cs"
    }
  },
  "publishOptions": {
    "include": [
      "web.config"
    ]
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": "1.0.4"
      }
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-preview4-final",
      "imports": "portable-net45+wp80+win8+wpa81+dnxcore50"
    }
  },
  "scripts": {
    "postpublish": "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
  },
  "runtimes": {
    "win7-x64": {},
    "win7-x86": {},
    "osx.10.10-x64": {},
    "osx.10.11-x64": {},
    "osx.10.12-x64": {},
    "ubuntu.14.04-x64": {},
    "ubuntu.15.04-x64": {}
  }
}